#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gaoyan/workspace/graphql_learning/node_modules/.pnpm/ruru@2.0.0-beta.30_graphile-config@0.0.1-beta.18_graphql@16.11.0/node_modules/ruru/dist/node_modules:/Users/gaoyan/workspace/graphql_learning/node_modules/.pnpm/ruru@2.0.0-beta.30_graphile-config@0.0.1-beta.18_graphql@16.11.0/node_modules/ruru/node_modules:/Users/gaoyan/workspace/graphql_learning/node_modules/.pnpm/ruru@2.0.0-beta.30_graphile-config@0.0.1-beta.18_graphql@16.11.0/node_modules:/Users/gaoyan/workspace/graphql_learning/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gaoyan/workspace/graphql_learning/node_modules/.pnpm/ruru@2.0.0-beta.30_graphile-config@0.0.1-beta.18_graphql@16.11.0/node_modules/ruru/dist/node_modules:/Users/gaoyan/workspace/graphql_learning/node_modules/.pnpm/ruru@2.0.0-beta.30_graphile-config@0.0.1-beta.18_graphql@16.11.0/node_modules/ruru/node_modules:/Users/gaoyan/workspace/graphql_learning/node_modules/.pnpm/ruru@2.0.0-beta.30_graphile-config@0.0.1-beta.18_graphql@16.11.0/node_modules:/Users/gaoyan/workspace/graphql_learning/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ruru@2.0.0-beta.30_graphile-config@0.0.1-beta.18_graphql@16.11.0/node_modules/ruru/dist/cli-run.js" "$@"
else
  exec node  "$basedir/../.pnpm/ruru@2.0.0-beta.30_graphile-config@0.0.1-beta.18_graphql@16.11.0/node_modules/ruru/dist/cli-run.js" "$@"
fi
